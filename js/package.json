{
  "name": "svelte-shared-store-custom-elements",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Svelte Rollup template with static asset hashing and minification",
  "author": "freshteapot (https://github.com/freshteapot)",
  "scripts": {
    "dev:hugo": "cd ../hugo && hugo server --disableFastRender --renderToDisk",
    "dev:js:shared": "rollup -c rollup.config.store.js -w",
    "dev:js:user": "rollup -c rollup.config.server.js -w",
    "dev:js:alist:v1": "rollup -c rollup.config.alist.v1.js -w",
    "dev:js:alist:v2": "rollup -c rollup.config.alist.v2.js -w",
    "dev:js:alist:v3": "rollup -c rollup.config.alist.v3.js -w",
    "dev:js:alist:v4": "rollup -c rollup.config.alist.v4.js -w",
    "dev:js:components": "concurrently \"npm run dev:js:user\" \"npm run dev:js:shared\" \"npm run dev:js:alist:v1\" \"npm run dev:js:alist:v2\" \"npm run dev:js:alist:v3\" \"npm run dev:js:alist:v4\"",
    "dev": "concurrently \"npm run dev:hugo\" \"npm run dev:js:components\"",
    "build:hugo": "cd ../hugo && hugo",
    "build:js:shared": "rollup -c rollup.config.store.js",
    "build:js:user": "rollup -c rollup.config.server.js",
    "build:js:alist:v1": "rollup -c rollup.config.alist.v1.js",
    "build:js:alist:v2": "rollup -c rollup.config.alist.v2.js",
    "build:js:alist:v3": "rollup -c rollup.config.alist.v3.js",
    "build:js:alist:v4": "rollup -c rollup.config.alist.v4.js",
    "build:js:components": "npm run build:js:user && npm run build:js:shared && npm run build:js:alist:v1 && npm run build:js:alist:v2 && npm run build:js:alist:v3 && npm run build:js:alist:v4",
    "build": "npm run build:js:components && npm run build:hugo"
  },
  "devDependencies": {
    "rollup": "^2.0.5",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "concurrently": "^5.1.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.4",
    "postcss": "^7.0.27",
    "postcss-import": "^12.0.1",
    "purgecss": "^2.1.0",
    "purgecss-from-svelte": "^2.0.2",
    "svelte-preprocess": "^3.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-output-manifest": "^1.0.2",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-postcss": "^2.1.1",
    "svelte": "^3.19.2",
    "fs-extra": "^9.0.0",
    "tachyons": "^4.11.1"
  }
}
