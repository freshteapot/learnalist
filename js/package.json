{
  "name": "Learnalist",
  "version": "0.0.1",
  "license": "MIT",
  "description": "The website for learnalist",
  "author": "freshteapot (https://github.com/freshteapot)",
  "type": "module",
  "scripts": {
    "dev:api-server": "cd ../ && make run-api-server",
    "dev:hugo": "cd ../hugo && hugo server -w --disableFastRender  --forceSyncStatic --renderToDisk",
    "dev:js:shared": "rollup -c rollup.config.store.js -w",
    "dev:js:main": "rollup -c rollup.config.main.js -w",
    "dev:js:alist:v1": "rollup -c rollup.config.alist.v1.js -w",
    "dev:js:alist:v2": "rollup -c rollup.config.alist.v2.js -w",
    "dev:js:alist:v3": "rollup -c rollup.config.alist.v3.js -w",
    "dev:js:alist:v4": "rollup -c rollup.config.alist.v4.js -w",
    "dev:js:alist:srs": "rollup -c rollup.config.alist.srs.js -w",
    "dev:js:alist:editor": "rollup -c rollup.config.alist.editor.js -w",
    "dev:js:components": "concurrently 'npm run dev:js:shared' 'npm run dev:js:main' 'npm run dev:js:alist:v1' 'npm run dev:js:alist:v2' 'npm run dev:js:alist:v3' 'npm run dev:js:alist:v4' 'npm run dev:js:alist:srs' 'npm run dev:js:alist:editor'",
    "dev": "concurrently \"npm run dev:hugo\" \"npm run dev:js:components\" \"npm run dev:api-server\"",
    "build:hugo": "cd ../hugo && hugo",
    "build:js:shared": "rollup -c rollup.config.store.js",
    "build:js:main": "rollup -c rollup.config.main.js",
    "build:js:alist:editor": "rollup -c rollup.config.alist.editor.js",
    "build:js:alist:v1": "rollup -c rollup.config.alist.v1.js",
    "build:js:alist:v2": "rollup -c rollup.config.alist.v2.js",
    "build:js:alist:v3": "rollup -c rollup.config.alist.v3.js",
    "build:js:alist:v4": "rollup -c rollup.config.alist.v4.js",
    "build:js:alist:srs": "rollup -c rollup.config.alist.srs.js",
    "build:js:components": "npm run build:js:main && npm run build:js:shared && npm run build:js:alist:v1 && npm run build:js:alist:v2 && npm run build:js:alist:v3 && npm run build:js:alist:v4 && npm run build:js:alist:srs && npm run build:js:alist:editor",
    "build": "npm run build:js:components && npm run build:hugo"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^5.0.1",
    "@sveltejs/gestures": "0.0.1",
    "autoprefixer": "^9.8.5",
    "concurrently": "^5.2.0",
    "fs-extra": "^9.0.1",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "proper-lockfile": "^4.1.1",
    "purgecss": "^2.3.0",
    "purgecss-from-svelte": "^2.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-output-manifest": "^1.0.2",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.24.0",
    "svelte-preprocess": "^3.9.12",
    "svelte-spa-router": "^2.2.0",
    "tachyons": "^4.12.0"
  },
  "dependencies": {
    "del": "^5.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  }
}
