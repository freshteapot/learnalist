{
  "name": "Learnalist",
  "version": "0.0.1",
  "license": "MIT",
  "description": "The website for learnalist",
  "author": "freshteapot (https://github.com/freshteapot)",
  "scripts": {
    "dev:api-server": "cd ../ && make run-api-server",
    "dev:hugo": "cd ../hugo && hugo server -w --disableFastRender  --forceSyncStatic --renderToDisk --verbose --verboseLog --debug",
    "dev:js:shared": "rollup -c rollup.config.shared.js -w",
    "dev:js:main": "rollup -c rollup.config.main.js -w",
    "dev:js:alist:v1": "rollup -c rollup.config.alist.v1.js -w",
    "dev:js:alist:v2": "rollup -c rollup.config.alist.v2.js -w",
    "dev:js:alist:v3": "rollup -c rollup.config.alist.v3.js -w",
    "dev:js:alist:v4": "rollup -c rollup.config.alist.v4.js -w",
    "dev:js:alist:srs": "rollup -c rollup.config.alist.srs.js -w",
    "dev:js:alist:editor": "rollup -c rollup.config.alist.editor.js -w",
    "dev:js:plank:v1": "rollup -c rollup.config.plank.v1.js -w",
    "dev:js:toolbox": "rollup -c rollup.config.toolbox.js -w",
    "dev:js:browser-extension:import-play": "rollup -c rollup.config.browser-extension.import-play.js -w",
    "dev:js:components": "concurrently 'npm run dev:js:shared' 'npm run dev:js:main' 'npm run dev:js:alist:v1' 'npm run dev:js:alist:v2' 'npm run dev:js:alist:v3' 'npm run dev:js:alist:v4' 'npm run dev:js:alist:srs' 'npm run dev:js:alist:editor' 'npm run dev:js:plank:v1' 'npm run dev:js:browser-extension:import-play' 'npm run dev:js:toolbox'",
    "dev": "concurrently \"npm run dev:hugo\" \"npm run dev:js:components\" \"npm run dev:api-server\"",
    "build:hugo": "cd ../hugo && hugo",
    "build:js:shared": "rollup -c rollup.config.shared.js",
    "build:js:main": "rollup -c rollup.config.main.js",
    "build:js:alist:editor": "rollup -c rollup.config.alist.editor.js",
    "build:js:alist:v1": "rollup -c rollup.config.alist.v1.js",
    "build:js:alist:v2": "rollup -c rollup.config.alist.v2.js",
    "build:js:alist:v3": "rollup -c rollup.config.alist.v3.js",
    "build:js:alist:v4": "rollup -c rollup.config.alist.v4.js",
    "build:js:alist:srs": "rollup -c rollup.config.alist.srs.js",
    "build:js:plank:v1": "rollup -c rollup.config.plank.v1.js",
    "build:js:toolbox": "rollup -c rollup.config.toolbox.js",
    "build:js:browser-extension:import-play": "rollup -c rollup.config.browser-extension.import-play.js",
    "build:js:components": "npm run build:js:main && npm run build:js:shared && npm run build:js:alist:v1 && npm run build:js:alist:v2 && npm run build:js:alist:v3 && npm run build:js:alist:v4 && npm run build:js:alist:srs && npm run build:js:alist:editor && npm run build:js:plank:v1 && npm run build:js:toolbox",
    "build": "npm run build:js:components && npm run build:hugo"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@sveltejs/gestures": "0.0.1",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "date-streaks": "^1.2.1",
    "dayjs": "^1.10.4",
    "del": "^6.0.0",
    "fs-extra": "^9.1.0",
    "humanize-duration": "^3.25.1",
    "postcss": "^8.2.10",
    "postcss-import": "^14.0.0",
    "proper-lockfile": "^4.1.2",
    "purgecss": "^4.0.2",
    "purgecss-from-svelte": "^2.0.2",
    "rollup": "^2.41.5",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-output-manifest": "^1.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.6.9",
    "svelte-spa-router": "^3.1.0",
    "tachyons": "^4.12.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "postcss-load-config": "^3.0.1",
    "rollup-plugin-css-only": "^3.1.0"
  }
}
